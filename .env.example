# Database Connection URL (dapatkan dari Neon)
DATABASE_URL="postgresql://[user]:[password]@[host]/[database]?sslmode=require"

# Cron Secret untuk security (ganti dengan random string)
CRON_SECRET="rahasia-unik-untuk-cron-job"

# Check Interval untuk auto-check (opsional)
# Format cron: "0 * * * *" = setiap jam
CHECK_INTERVAL="0 * * * *"
